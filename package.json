{
    "name": "wordpress-salts",
    "main": "./lib/wordpress-salts",
    "version": "0.1.0",
    "description": "Inserts WordPress salts in PHP, YAML, DotEnv or JSON files",
    "scripts": {
        "build": "tsc --pretty --project ./",
        "dev": "tsc --watch --pretty --project ./",
        "lint": "tslint --project ./",
        "prepublishOnly": "npm run build",
        "precommit": "npm run lint",
        "test": "npm run lint"
    },
    "keywords": [
        "wordpress",
        "wordpress salts",
        "cryptography"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/idleberg/atom-wordpress-salts"
    },
    "homepage": "https://atom.io/packages/wordpress-salts",
    "bugs": {
        "url": "https://github.com/idleberg/atom-wordpress-salts/issues"
    },
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "activationCommands": {
        "atom-workspace": [
            "WordPress-Salts:Insert"
        ]
    },
    "dependencies": {
        "wp-salts": "^0.3.1"
    },
    "devDependencies": {
        "@types/atom": "^1.31.1",
        "@types/node": "^11.13.4",
        "husky": "^1.3.1",
        "tslint": "^5.15.0",
        "typescript": "^3.4.3"
    }
}
